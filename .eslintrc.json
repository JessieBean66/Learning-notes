{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "semi": "error"
  },
  "overrides": [
    {
        "files": ["*.md"],
        "processor": "a-plugin/markdown"
    }
  ],
  "eslintConfig": {
    "env": {
        "browser": true,
        "node": true
    }
  }
}